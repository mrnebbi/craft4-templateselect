{
    "name": "superbig/craft3-templateselect",
    "description": "A fieldtype that allows you to select a template from a dropdown.",
    "type": "craft-plugin",
    "version": "2.0.4",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "template select"
    ],
    "support": {
        "docs": "https://github.com/breizhwave/craft4-templateselect/blob/master/README.md",
        "issues": "https://github.com/breizhwave/craft4-templateselect/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Superbig",
            "homepage": "https://superbig.co"
        } ,   {
            "name": "breizhwave",
            "homepage": "https://wave.bzh"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "breizhwave\\templateselect\\": "src/"
        }
    },
    "extra": {
        "name": "Template Select",
        "handle": "template-select",
        "schemaVersion": "2.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/breizhwave/craft4-templateselect/master/CHANGELOG.md",
        "class": "breizhwave\\templateselect\\TemplateSelect"
    }
}
